# Production Environment Variables for Phobetron
# Copy this file to .env and update with your production values

# Database
POSTGRES_PASSWORD=change_this_to_a_secure_password

# Backend Security
SECRET_KEY=change_this_to_a_long_random_string_for_jwt_tokens

# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME=Phobetron - Celestial Signs

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,https://your-production-domain.com

# Environment
ENVIRONMENT=production

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:8020
